generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Plan {
  id        Int      @id @default(autoincrement())
  name      String
  budget    Float
  status    String   @default("DRAFT") // DRAFT, RUNNING, STOPPED
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

